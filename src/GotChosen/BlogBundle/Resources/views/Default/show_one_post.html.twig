{% extends "::base.html.twig" %}
{% block title %}GotChosen | Add Post{% endblock %}
{% block body %}
    <div class="container">
        {% include 'GotChosenBlogBundle:Default:Blocks/nav.html.twig' %}

        <!-- Main component for a primary marketing message or call to action -->
        <div class="jumbotron">

<p>ID={{ posting.id }} Title={{ posting.postTitle }} CreatedAt = {{ posting.createdAt|date('l, F jS, Y h:i:s A') }}
<br>
{% for tag in posting.tags %}
<br>ID={{ tag.id }} Name={{ tag.name }}
{% endfor %}

</p>
</div>
        </div>
{% endblock %}